{
  "version": 2,
  "name": "feedback-collection-frontend",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "installCommand": "npm install --legacy-peer-deps",
  "buildCommand": "npm run build",
  "env": {
    "VITE_API_URL": "https://feedback-collection-t8g8-dhruv-tanejas-projects.vercel.app"
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://feedback-collection-t8g8-dhruv-tanejas-projects.vercel.app/api/$1",
      "headers": {
        "Access-Control-Allow-Origin": "https://feedback-collection-1fzp-61jzczzm-dhruv-tanejas-projects.vercel.app",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Origin, X-Requested-With, Content-Type, Accept, Authorization",
        "Access-Control-Allow-Credentials": "false"
      }
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
